<%= simple_form_for(@post) do |f|%>
    <%= f.input :title%>
    <%= f.input :body%>
    <%= f.input :image, as: :file %>
    <%= f.input :file, as: :file %>
    <%= f.input :clip, as: :file %>
    <%= f.input :thumbnail, as: :file, input_html: { multiple: true } %>
    <%= f.button :submit %>
<%end%>